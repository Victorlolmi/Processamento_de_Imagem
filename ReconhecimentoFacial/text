from pytube import YouTube
from pathlib import Path
from os import *
from pywebio.input import  *
from pywebio.output import *

def video_download():

    while True:

        video_link = input("Link:")

        if video_link.split("//")[0] == "https:":

            put_text("Fazendo download....".title()).style('color: red; font-size: 50px')
            video_url = YouTube(video_link)
            video = video_url.streams.get_highest_resolution()
            path_to_download = (r"ReconhecimentoFacial\download")
            video.download(path_to_download)
            put_text("Video baixado com sucesso...".title()).style('color: blue; font-size:50px')
            startfile(r"ReconhecimentoFacial\\download")

video_download()

